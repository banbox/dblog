DB_NAME=squid
DB_PORT=5432
DB_HOST=localhost
DB_USER=postgres
DB_PASS=123
GQL_PORT=4350

# amberink contract address
BLOG_HUB_ADDRESS=0xa60eE127a075B62C3B5513F99CF91A15Cea238b6

# SessionKeyManager contract address
SESSION_KEY_MANAGER_ADDRESS=0x67284350C1B935Ca80A1e6658f472F3156cF7313

# Finality confirmation (blocks to wait before processing)
# Local Anvil: 1, OP Mainnet/Sepolia: 75+
FINALITY_CONFIRMATION=1

# Block range start (useful for resyncing from specific block)
BLOCK_RANGE_FROM=0

# Subsquid Gateway URL (optional, leave empty for RPC-only mode)
# OP Mainnet: https://v2.archive.subsquid.io/network/optimism-mainnet
# OP Sepolia: https://v2.archive.subsquid.io/network/optimism-sepolia
# GATEWAY_URL=

# JSON-RPC node endpoints (wss or https)
# Use private endpoints in production! If deploying to Cloud:
# - use our RPC proxy service: https://docs.subsquid.io/cloud/resources/rpc-proxy/, OR
# - save your private url as a secret: https://docs.subsquid.io/cloud/resources/env-variables/#secrets
# RPC_ETH_HTTP=https://rpc.ankr.com/eth

# Optimism Sepolia (uncomment for testnet)
# RPC_ETH_HTTP=https://sepolia.optimism.io

# Local Anvil (for local development)
RPC_ETH_HTTP=http://localhost:8545

RPC_RATE_LIMIT=10

# Uncommenting this line enables the debug mode
# More info at https://docs.subsquid.io/basics/logging/
#SQD_DEBUG=*
